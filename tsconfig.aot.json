{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "declaration": true,
    "declarationDir": "out/aot",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": ".compiled",
    "typeRoots": [ "./node_modules/@types" ],
    "types": [
      "core-js",
      "node"
    ],
    "lib": [
      "es2015",
      "es2016",
      "es6",
      "es7",
      "dom"
    ],
    "baseUrl": "."
  },
  "angularCompilerOptions": {
    "debug": true,
    "genDir": ".aot",
    "skipMetadataEmit": false
  },
  "compileOnSave": false,
  "files": [
    "src/dynamic.forms.module.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "out"
  ]
}