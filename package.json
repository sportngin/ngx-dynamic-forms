{
  "name": "ng-dynamic-forms",
  "version": "0.0.11",
  "description": "",
  "author": "SI Play",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:siplay/ng-dynamic-forms.git"
  },
  "main": "dist/dynamic.forms.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:templates": "gulp build-templates",
    "build:ngc": "ngc -p ./src/tsconfig.json",
    "build:webpack": "webpack --config webpack.config.js --display-chunks --display-reasons --display-modules",
    "build": "npm run prebuild; npm run build:ngc; npm run build:webpack; npm run build:templates",
    "prepublishOnly": "npm run build",
    "test": "rimraf .tmp && mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "test:watch": "rimraf .tmp && mocha-webpack --opts config/mocha/mocha-webpack.opts --watch",
    "test:report": "rimraf .tmp && mocha-webpack --opts config/mocha/mocha-webpack.opts --reporter=xunit --reporter-options output=tests-report.xml",
    "test:coverage": "rimraf .tmp && nyc --reporter=text --reporter=cobertura mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "test:server": "webpack-dev-server --config config/webpack.test.browser.js --inline --progress --port 8888",
    "dev": "webpack-dev-server --config config/webpack.dev.js --history-api-fallback true --inline --progress --port 8888"
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "@angular/common": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.0.1",
    "zone.js": "^0.8.4"
  },
  "dependencies": {
    "lodash": "^4.17.0",
    "moment": "^2.18.0",
    "ngx-color-picker": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/compiler-cli": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/router": "^4.1.0",
    "@types/chai": "^3.5.2",
    "@types/core-js": "0.9.41",
    "@types/lodash": "4.14.64",
    "@types/mocha": "^2.2.41",
    "@types/node": "7.0.22",
    "@types/sinon": "^2.2.1",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.3",
    "bootstrap": "^4.0.0-alpha.6",
    "chai": "4.0.0",
    "css-loader": "0.28.3",
    "extract-loader": "0.1.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "font-awesome-sass-loader": "2.0.1",
    "gulp": "^3.9.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "^11.0.0",
    "mocha": "^3.4.1",
    "mocha-loader": "^1.1.1",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.2",
    "phantomjs-prebuilt": "^2.1.14",
    "pug": "2.0.0-rc.1",
    "pug-cli": "1.0.0-alpha6",
    "pug-html-loader": "^1.1.4",
    "pug-loader": "2.3.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.0.1",
    "sass-loader": "^6.0.5",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.1",
    "systemjs": "^0.20.12",
    "through2": "^2.0.3",
    "to-string-loader": "^1.1.5",
    "typescript": "2.3.3",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "^2.4.5",
